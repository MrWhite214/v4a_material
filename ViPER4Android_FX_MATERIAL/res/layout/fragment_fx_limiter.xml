<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="72.0dp"
            android:paddingLeft="16.0dip"
            android:paddingRight="16.0dip">

            <TextView
                android:id="@id/id_tv_output_pan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:text="@string/pref_output_pan"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="16dp" />

            <CheckBox
                android:id="@id/id_output_pan_enable"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@id/id_tv_output_pan"
                android:layout_alignParentRight="true"
                android:button="@null" />

            <TextView
                android:id="@id/id_tv_output_pan_val"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="16.0dip"
                android:layout_below="@+id/id_tv_output_pan"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <SeekBar
                android:id="@id/id_sb_output_pan"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:focusable="true"
                android:max="200"
                android:layout_alignBottom="@+id/id_tv_output_pan_val"
                android:layout_toRightOf="@+id/id_tv_output_pan_val"
                android:layout_toEndOf="@+id/id_tv_output_pan_val" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.100000024dip"
            android:background="@color/blacktransparent" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="72.0dp"
            android:paddingLeft="16.0dip"
            android:paddingRight="16.0dip">

            <TextView
                android:id="@id/id_tv_output_gain"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:text="@string/pref_output_gain"
                android:layout_above="@+id/id_tv_output_gain_val"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <CheckBox
                android:id="@id/id_output_gain_enable"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@id/id_tv_output_gain"
                android:layout_alignParentRight="true"
                android:button="@null"
                android:checked="true" />

            <TextView
                android:id="@id/id_tv_output_gain_val"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginBottom="16dp"
                android:paddingLeft="16.0dip"/>

            <SeekBar
                android:id="@id/id_sb_output_gain"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="17dp"
                android:layout_toEndOf="@id/id_tv_output_gain_val"
                android:layout_toRightOf="@id/id_tv_output_gain_val"
                android:focusable="true"
                android:max="210" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.100000024dip"
            android:background="@color/blacktransparent" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="72.0dp"
            android:paddingLeft="16.0dip"
            android:paddingRight="16.0dip">

            <TextView
                android:id="@id/id_tv_limiter_threshold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@+id/id_sb_limiter_threshold"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:gravity="center_vertical"
                android:text="@string/pref_fx_limiter_threshold"/>

            <CheckBox
                android:id="@id/id_limiter_threshold_enable"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@id/id_tv_limiter_threshold"
                android:layout_alignParentRight="true"
                android:button="@null"
                android:checked="true" />

            <TextView
                android:id="@id/id_tv_limiter_threshold_val"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/id_tv_limiter_threshold"
                android:paddingLeft="16.0dip"/>

            <SeekBar
                android:id="@id/id_sb_limiter_threshold"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="17dp"
                android:layout_toEndOf="@+id/id_tv_limiter_threshold_val"
                android:layout_toRightOf="@+id/id_tv_limiter_threshold_val"
                android:focusable="true"
                android:max="50" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.100000024dip"
            android:background="@color/blacktransparent" />
    </LinearLayout>
</ScrollView>
